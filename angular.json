{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {

    "@ng-queuex/core": {
      "projectType": "library",
      "root": "projects/ng-queuex/core",
      "sourceRoot": "projects/ng-queuex/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-queuex/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-queuex/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ng-queuex/core/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@ng-queuex/template": {
      "projectType": "library",
      "root": "projects/ng-queuex/template",
      "sourceRoot": "projects/ng-queuex/template/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-queuex/template/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-queuex/template/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ng-queuex/template/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "sample-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/sample-app",
      "sourceRoot": "projects/sample-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/sample-app/src/main.ts",
            "tsConfig": "projects/sample-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/sample-app/public"
              }
            ],
            "styles": [
              "projects/sample-app/src/styles.scss"
            ],
            "server": "projects/sample-app/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/sample-app/src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "sample-app:build:production"
            },
            "development": {
              "buildTarget": "sample-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/sample-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/sample-app/public"
              }
            ],
            "styles": [
              "projects/sample-app/src/styles.scss"
            ]
          }
        }
      }
    },
    "@ng-queuex/signals": {
      "projectType": "library",
      "root": "projects/ng-queuex/signals",
      "sourceRoot": "projects/ng-queuex/signals/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-queuex/signals/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-queuex/signals/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ng-queuex/signals/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
