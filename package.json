{
  "name": "@ng-queuex",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "build:core": "ng build @ng-queuex/core",
    "build:template": "ng build @ng-queuex/template",
    "build:all": "npm run build:core && npm run build:template",
    "pack:core": "cd dist/ng-queuex/core && npm pack && copy *.tgz ..\\..\\.. && del *.tgz",
    "pack:template": "cd dist/ng-queuex/template && npm pack && copy *.tgz ..\\..\\.. && del *.tgz",
    "pack:all": "npm run pack:core && npm run pack:template",
    "release": "npm run build:all && npm run pack:all",
    "serve:ssr:sample-app": "node dist/sample-app/server/server.mjs"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.8.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-preset-angular": "^15.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2"
  }
}